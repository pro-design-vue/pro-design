{
  "name": "pro-design-vue",
  "version": "0.0.0-dev.1",
  "description": "A Component Library for Vue 3",
  "keywords": [
    "pro design",
    "pro design vue",
    "component library",
    "ui framework",
    "ui",
    "ant-design-vue",
    "vue"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs",
      "require": "./lib/index.js"
    },
    "./global": {
      "types": "./global.d.ts"
    },
    "./es": {
      "types": "./es/index.d.ts",
      "import": "./es/index.mjs"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js"
    },
    "./es/*.mjs": {
      "types": "./es/*.d.ts",
      "import": "./es/*.mjs"
    },
    "./es/*": {
      "types": [
        "./es/*.d.ts",
        "./es/*/index.d.ts"
      ],
      "import": "./es/*.mjs"
    },
    "./lib/*.js": {
      "types": "./lib/*.d.ts",
      "require": "./lib/*.js"
    },
    "./lib/*": {
      "types": [
        "./lib/*.d.ts",
        "./lib/*/index.d.ts"
      ],
      "require": "./lib/*.js"
    },
    "./*": "./*"
  },
  "unpkg": "dist/index.full.js",
  "jsdelivr": "dist/index.full.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pro-design-vue/pro-design"
  },
  "publishConfig": {
    "access": "public"
  },
  "style": "dist/index.css",
  "sideEffects": [
    "dist/*",
    "theme-chalk/**/*.css",
    "theme-chalk/src/**/*.scss",
    "es/components/*/style/*",
    "lib/components/*/style/*"
  ],
  "author": {
    "name": "shene",
    "email": "shen.it@foxmail.com",
    "url": "https://github.com/pro-desion-vue"
  },
  "peerDependencies": {
    "ant-design-vue": "^4.2.0",
    "vue": "^3.5.0"
  },
  "dependencies": {
    "@ant-design/colors": "^7.2.1",
    "@ant-design/icons-vue": "^7.0.1",
    "@vueuse/core": "^13.3.0",
    "cropperjs": "^1.6.1",
    "dayjs": "^1.11.13",
    "lodash-es": "^4.17.21",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@types/node": "*",
    "ant-design-vue": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:",
    "vue-types": "catalog:",
    "scroll-into-view-if-needed": "catalog:"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}